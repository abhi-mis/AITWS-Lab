        "import numpy as np\n",
        "a=np.random.randint(10,30,(3,3))\n",
        "b=np.ones([3,3],dtype='int');\n",
        "c=a+b;\n",
        "d=c\n",
        "print(\"Matrix A\\n\",a,\"\\n\\nMatrix B\\n\",b,\"\\n\\nMatrix C\\n\",c)\n",
        "c=np.array(c,dtype='complex')\n",
        "d=d.astype('complex')\n",
        "print(\"Matrix C\\n\",c,\"\\n\\nMatrix D\\n\",d)"
      ]
